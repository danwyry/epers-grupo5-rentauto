<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
  <class name="org.unq.epers.grupo5.rentauto.model.Auto" table="AUTO">
  	<id name='id' column='ID' type='java.lang.Integer' >
      <generator class="identity" />
  	</id>

    <property name="marca" type="string">
      <column name="MARCA" length="20" not-null="true" unique="false" />
    </property>
                
    <property name="modelo" type="string">
      <column name="MODELO" length="20" not-null="true" unique="false" />
    </property>
    
    <property name="anio" type="int">
       <column name="ANIO" not-null="false" unique="false"/>
    </property>

    <property name="patente" type="int">
       <column name="PATENTE" not-null="false" unique="false"/>
    </property>

    <property name="costoBase" type="int">
       <column name="COSTO_BASE" not-null="false" unique="false"/>
    </property>
    
    <property name="categoria" type="int">
       <column name="CATEGORIA" not-null="false" unique="false"/>
    </property>

    <property name="ubicacionInicial" type="int">
       <column name="UBICACION_INICIAL" not-null="false" unique="false"/>
    </property>
    
	<list name="reservas" table="PosicionFormacionStrategy" >
		<key column="formacionStrategy"></key>
		<index column="indice" />
		<element type="Posicion" column="posicion"/>
    </list>
    


  </class>
</hibernate-mapping>